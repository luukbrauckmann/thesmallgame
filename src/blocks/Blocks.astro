---
interface Props {
  blocks: any[];
}

const { blocks } = Astro.props;
---

{blocks.map(async (block) => {
  const name = block.__typename.replace('Record', 'Block');
  const Block = await import(`../blocks/${name}/${name}.tsx`)
    .then((module) => module.default);
  return <Block {...block} />;
})}
